{
  "name": "hopem-demo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:lw": "turbo run dev --filter=landing-web",
    "dev:wa": "turbo run dev --filter=web-app",
    "dev:ma": "turbo run dev --filter=mobile-app"
  },
  "devDependencies": {
    "config": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.18"
}
